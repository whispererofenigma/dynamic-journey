{
    "version": 2,
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
    
      {
        "src": "/(.*)",
        "dest": "/backend/index.js"
      }
    ]
  }
  